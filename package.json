{
  "name": "rounds-for-luxoft",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/katepimenova/rounds-for-luxoft.git"
  },
  "engines": {
    "node": ">=0.10.40"
  },
  "scripts": {
    "start": "gulp dev-server",
    "lint": "npm run lintspaces && npm run eslint",
    "eslint": "eslint .",
    "lintspaces": "find static -name '*.less' -o -name '*.css' | xargs lintspaces -nt -d spaces -i 'js-comments' -s 2 -l 2",
  },
  "dependencies": {
    "backbone": "1.3.3",
    "jquery": "3.0.0",
    "less": "2.6.1",
    "less-loader": "2.2.1",
    "webpack": "1.13.1",
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-simple-amd": "0.3.0",
    "eslint": "2.11.1",
    "webpack-dev-server": "1.14.0"
  }
}
